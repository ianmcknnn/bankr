<h1>Welcome, <%= @user.username %>! </h1>
<h3>Accounts</h3>
<ul>
  <%@user.accounts.each do |account|%>
    <li><%= link_to account.bank.name, account_path(account) %><br><%=" balance: #{account.balance}" %></li>
  <% end %>
</ul>

<%= button_to "Create new account", new_account_path, method: :get%>
<%= button_to "Edit user", edit_user_path(@user.id), method: :get %>
